<div class="header">
  <%= render "meetings/menu_bar"%>
</div>
<div class="main">
<div class="clock-all">
  <div class="clock">
    <div class="clock-frame",id="clock-frame">
      <span id="clock-date"></span>
      <span id="clock-time"></span>
    </div>    
    <div class="stamping">
      <%= form_with url:"/work_times", method: :post, id: "form" do |f|%>
        <%= f.submit "出勤/退勤", class:"stamping-btn",id:"submit"%>
      <% end %>
    </div>
  </div>
    <div id="list"></div>

  <div class="working-data">
  <% @work_times.each do |work_time|%>
    <div class="work-time" data-id = <%= work_time.id%>>
      <div class="work-time-date">
        <%= work_time.time.strftime("%Y-%m-%d %H:%M:%S") %>
      </div>
    </div>
  <% end %>
  </div>
</div>
</div>



